{
  "name": "vue-unstated",
  "version": "0.0.1",
  "description": "A tiny state management library for Vue Composition API.",
  "repository": "ssh://git@github.com/resessh/vue-unstated.git",
  "homepage": "https://github.com/resessh/vue-unstated#readme",
  "bugs": {
    "url": "https://github.com/resessh/vue-unstated/issues"
  },
  "author": "resessh <info@resessh.net>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "state-management",
    "composition-api",
    "function-api"
  ],
  "source": "src/vue-unstated.ts",
  "main": "lib/vue-unstated.js",
  "module": "lib/vue-unstated.mjs",
  "unpkg": "lib/vue-unstated.umd.js",
  "types": "lib/vue-unstated.d.ts",
  "files": [
    "lib",
    "src",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "rm -rf dist && microbundle src/index.ts --external @vue/composition-api --globals @vue/composition-api=vueCompositionApi --strict -o lib",
    "prettier": "prettier --write ./**/*.ts"
  },
  "devDependencies": {
    "@vue/composition-api": "^0.3.4",
    "microbundle": "^0.11.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.3.4"
  }
}
